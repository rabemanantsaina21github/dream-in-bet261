<!DOCTYPE html>
<html>
    <head>
        {{ include('home/layout/header.html.twig') }}

        {% block seo %}
        <base href="{{ path('Accueil') }}">
        <meta name="keywords" content="">
        <meta name="x-ua-compatible" content="IE=edge, chrome=1">
        <meta name="description" content="">
        <meta property="og:title" content="DreamInBet261.mg | Accueil">
        <meta property="og:type" content="page" >
        <meta property="og:url" content="{{ path('Accueil') }}">
        <meta property="og:image" content="">
        <meta property="og:description" content="">
        <meta property="og:video" content="">
        <meta property="og:locale" content="fr_FR">
        <meta name="twitter:card" content="summary_large_image">
        <meta name="twitter:url" content="{{ path('Accueil') }}">
        <meta name="twitter:title" content="DreamInBet261.mg | Accueil">
        <meta name="twitter:description" content="">
        <meta name="author" content="Antonio RABEMANANTSAINA, née le 29 Août 1994 à Fenerive-est. Fils de RABEMANANTSAINA Christophe et de LALAHY Julienne">
        <meta name="copyright" content="© DreamInBet261 2021">
        {% endblock %}

        <script>
            (function (w, d, s, l, i) {
                w[l] = w[l] || []; w[l].push({
                    'gtm.start':
                        new Date().getTime(), event: 'gtm.js'
                }); var f = d.getElementsByTagName(s)[0],
                    j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
                        'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
            })(window, document, 'script', 'dataLayer', 'GTM-NVBJV6Q');
        </script>
        
        <title>{% block title %}Dream'in Bet{% endblock %}</title>
        <link href="/fontawesome/css/all.min.css" rel="stylesheet">
        <link href="/icomoon/css/icomoon.css" rel="stylesheet">
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
            <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
        {% endblock %}
        <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    </head>
    <body id="{% block body_id %}{% endblock %}" class="{% block class %}{% endblock %}">
        <noscript>
            <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NVBJV6Q" height="0" width="0" style="display:none;visibility:hidden"></iframe>
        </noscript>
        {{ include('home/composants/header.html.twig') }}
        <main id="main" class="main">
            <div class="container">
                {% block flash %}
                    {% for flash_message in app.session.flashbag.get('success') %}
                    <div class="alert alert-success data-dismiss mt-2" role="alert">
                        <a href="" class="close mt-moins-cinq" data-dismiss="alert" aria-label="close">x</a>
                        {{ flash_message|raw }}
                    </div>
                    {% endfor %}
                    {% for flash_message in app.session.flashbag.get('info') %}
                    <div class="alert alert-info data-dismiss mt-2" role="alert">
                        <a href="" class="close mt-moins-cinq" data-dismiss="alert" aria-label="close">x</a>
                        {{ flash_message|raw }}
                    </div>
                    {% endfor %}
                    {% for flash_message in app.session.flashbag.get('warning') %}
                    <div class="alert alert-warning data-dismiss mt-2" role="alert">
                        <a href="" class="close mt-moins-cinq" data-dismiss="alert" aria-label="close">x</a>
                        {{ flash_message|raw }}
                    </div>
                    {% endfor %}
                    {% for flash_message in app.session.flashbag.get('danger') %}
                    <div class="alert alert-danger data-dismiss mt-2" role="alert">
                        <a href="" class="close mt-moins-cinq" data-dismiss="alert" aria-label="close">x</a>
                        {{ flash_message|raw }}
                    </div>
                    {% endfor %}
                {% endblock %}
            </div>
            {% block page_content %}{% endblock %}
            {% block pagetext_content %}{% endblock %}
            {{ include('home/composants/footer.html.twig') }}
        </main>
        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="{{ asset('asset/libs/js/jquery-3.5.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('asset/libs/js/popper-1.16.0.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('asset/libs/js/bootstrap.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('javascripts/home.js') }}" type="text/javascript"></script>
            <script src="{{ asset('javascripts/script.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
